<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="userService" class="com.netcracker.crm.services.impl.UserServiceImpl">
        <property name="userDao" ref="userDao"/>
    </bean>

    <bean id="productService" class="com.netcracker.crm.services.impl.ProductServiceImpl">
        <property name="entityDao" ref="entityDao"/>
    </bean>
    <bean id="orderService" class="com.netcracker.crm.services.impl.OrderServiceImpl">
        <property name="entityDao" ref="entityDao"/>
    </bean>
    <bean id="cartService" class="com.netcracker.crm.services.impl.CartServiceImpl">
        <property name="entityDao" ref="entityDao"/>
    </bean>

</beans>